{
    "name": "depsimon/laravel-alerts",
    "description": "Easy alerts with Laravel & Vue.js",
    "authors": [
        {
            "name": "Simon Depelchin",
            "email": "simon@webartisan.be"
        }
    ],
    "require": {
        "illuminate/support": "~5.0"
    },
    "autoload": {
        "psr-0": {
            "Depsimon\\Alerts": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Depsimon\\Alerts\\AlertsServiceProvider"
            ],
            "aliases": {
                "Alert": "Depsimon\\Alerts\\AlertFacade"
            }
        }
    }
}
